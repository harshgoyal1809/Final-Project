
@model Return_Order_Portal.Models.Submission

@{
    ViewData["Title"] = "ProcessResponse";
}


<!DOCTYPE html>
<html>
<head>
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" rel="stylesheet" />
</head>
<body style="background-color: black">
    <h1 style="font-family: 'Times New Roman', cursive; color: aqua; position: relative;">Processed successfully!</h1>
    <h2 style="font-family: 'Segoe UI', cursive; color: aqua; position: relative;">Your remaning balance is @Model.Result</h2>
    <div class="form-group">
        <div class="btn btn-warning" style="color:blue"> <a asp-controller="User" asp-action="Logout">Log Out</a></div>
    </div>
    <img src="~/images/all done.png" style="margin:auto"/>
</body>
</html>




